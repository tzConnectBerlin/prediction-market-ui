    "react-icons": "^4.2.0",
    "react-query": "^3.13.8",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-toast-notifications": "^2.4.0",
    "redux": "^4.0.5",
    "yup": "^0.32.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "commit": "cz",
    "postinstall": "husky install",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "format": "prettier --write src/**/*.ts{,x}",
    "tsc:compile": "tsc",
    "lint": "yarn tsc:compile && eslint src/**/*.ts{,x}",
    "chromatic": "chromatic",
    "clean:test-results": "rimraf ./test-results/junit && mkdir -p ./test-results/junit",
    "test-ci": "yarn clean:test-results && yarn test --ci --coverage --watchAll=false --reporters=default --reporters=jest-junit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@emotion/jest": "^11.3.0",
    "@storybook/addon-actions": "^6.2.7",
    "@storybook/addon-essentials": "^6.2.7",
    "@storybook/addon-links": "^6.2.7",
    "@storybook/node-logger": "^6.2.7",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@testing-library/dom": "^7.30.3",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.3",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.1",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-test-renderer": "^17.0.1",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-loader": "8.1.0",
    "canvas": "^2.7.0",
    "chromatic": "^5.7.1",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest-junit": "^12.0.0",
    "lint-staged": "^10.5.4",
    "pinst": "^2.1.1",
    "prettier": "^2.2.1",
    "react-scripts": "^4.0.2",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.4",
    "web-vitals": "^1.0.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.ts{,x}": [
      "yarn format",
      "eslint src/**/*.ts{,x}"
    ]
  },
  "jest": {
    "clearMocks": true,
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.{d.ts,stories.tsx}"
    ]
  },
  "jest-junit": {
    "outputDirectory": "./test-results/junit",
    "outputName": "results.xml"
  }
}