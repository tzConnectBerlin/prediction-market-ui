  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "yarn build && source-map-explorer 'build/static/js/*.js'",
    "commit": "cz",
    "postinstall": "husky install",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "format": "prettier --write src/**/*.ts{,x}",
    "tsc:compile": "tsc",
    "lint": "yarn tsc:compile && eslint src/**/*.ts{,x}",
    "chromatic": "chromatic",
    "clean:test-results": "rimraf ./test-results/junit && mkdir -p ./test-results/junit",
    "test-ci": "yarn clean:test-results && yarn test --ci --maxWorkers=2 --coverage --watchAll=false --reporters=default --reporters=jest-junit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "not IE 11"
    ]
  },
  "devDependencies": {
    "@axe-core/react": "^4.2.1",
    "@babel/core": "^7.14.3",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@emotion/jest": "^11.3.0",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/node-logger": "^6.3.4",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@testing-library/dom": "^7.31.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/ramda": "^0.27.40",
    "@types/react": "^17.0.6",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-test-renderer": "^17.0.1",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "babel-loader": "8.1.0",
    "canvas": "^2.8.0",
    "chromatic": "^5.8.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest-junit": "^12.0.0",
    "lint-staged": "^11.1.1",
    "pinst": "^2.1.6",
    "prettier": "^2.3.0",
    "react-scripts": "^4.0.3",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "source-map-explorer": "^2.5.2",
    "styled-components": "^5.3.0",
    "typescript": "^4.2.4",
    "webpack": "4.44.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.ts{,x}": [
      "yarn format",
      "eslint src/**/*.ts{,x}"
    ]
  },
  "jest": {
    "clearMocks": true,
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.{d.ts,stories.tsx}"
    ]
  },
  "jest-junit": {
    "outputDirectory": "./test-results/junit",
    "outputName": "results.xml"
  }
}